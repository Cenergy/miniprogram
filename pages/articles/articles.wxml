<view style='display:flex;flex-direction:column' wx:for="{{logs}}" wx:key="key" wx:for-item="value" bindtap="bindViewTap" id="{{value.id}}">
  <view style='border-bottom:5px solid #EFEFF3;margin-bottom:10px;padding:5px'>
  <view  class="userinfo">
   <text class="userinfo-nickname"></text>
  </view>
    <view>
      <image src="https://cdn.v2ex.co/site/logo@2x.png" style="height:20px;width:20px;color:#888888" />
      <text style='font-size:14px'></text>
    </view>
    <view>
      <text style='font-size:20px;color:#353535'>{{value.title}}</text>
    </view>
    <view style='display:flex;justify-content:space-between'>
      <text style='font-size:16px;color:#888888'>{{value.id}}</text>
      <text style='font-size:16px;color:#888888'></text>
    </view>
  </view>
</view>